                    </div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <label class="col-form-label">Tipo Sub Categoria</label>
                            <select id="selectincidencia" class="form-select form-control">
                            </select>
                        </div>

                    </div>
                }


